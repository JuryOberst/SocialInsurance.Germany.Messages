{
  "version": "1.0.0-*",
  "authors": [ "DATALINE GmbH & Co. KG" ],

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ],
        "debugType": "full"
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none"
      }
    }
  },

  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../Dataline.ruleset",
      "/r:deuev16=../../packages/Dsrv.Kernpruefung.Deuev-1.6/1.6.1.7/lib/net40/Dsrv.Kernpruefung.Deuev-1.6.dll",
      "/r:deuev17=../../packages/Dsrv.Kernpruefung.Deuev-1.7/1.7.0.7/lib/net40/Dsrv.Kernpruefung.Deuev-1.7.dll",
