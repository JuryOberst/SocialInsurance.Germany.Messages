<?xml version="1.0" encoding="utf-8" ?>
<beanio xmlns="http://www.beanio.org/2015/06">
  <import resource="resource:SocialInsurance.Germany.Messages.Mappings.DBAZ.xml, SocialInsurance.Germany.Messages.Mappings"/>
  <import resource="resource:SocialInsurance.Germany.Messages.Mappings.DBEZ.xml, SocialInsurance.Germany.Messages.Mappings"/>
  <import resource="resource:SocialInsurance.Germany.Messages.Mappings.DBFE.xml, SocialInsurance.Germany.Messages.Mappings"/>
  <import resource="resource:SocialInsurance.Germany.Messages.Mappings.Types.xml, SocialInsurance.Germany.Messages.Mappings"/>
  <template name="DSAE-v01">
    <field name="KE" literal="DSAE" length="4" rid="true" required="true"/>
    <field name="VF" length="5" required="true"/>
    <field name="BBNRAB" length="15" required="true"/>
    <field name="BBNREP" length="15" required="true"/>
    <field name="VERNR" length="2" literal="01" required="true" type="int" format="00"/>
    <field name="ED" length="20" required="true" format="yyyyMMddHHmmssffffff"/>
    <field name="FEKZ" regex="[0-4]" length="1" required="true" />
    <field name="FEAN" length="1" required="true" type="int"/>
    <field name="VSNR" length="12" required="true"/>
    <field name="VSTR" length="2" required="true"/>
    <field name="BBNRVU" length="15" required="true"/>
    <field name="AZVU" length="20" required="true"/>
    <field name="RESERVE1" length="58" required="false" ignore="true"/>
    <field name="MMAZ" length="1" required="true" typeHandler="jn"/>
    <field name="MMEZ" length="1" required="true" typeHandler="jn"/>
    <field name="RESERVE2" length="8" required="false" ignore="true"/>
    <field name="KENNZUE" length="1" required="true"/>
    <field name="RESERVE3" length="5" required="false" ignore="true"/>
    <field name="VERNRKP" length="2" required="true"/>
    <field name="RESERVE4" length="2" required="false" ignore="true"/>
    <segment name="DBAZ" class="SocialInsurance.Germany.Messages.Pocos.DBAZ, SocialInsurance.Germany.Messages.Pocos" occursRef="MMAZ" collection="list">
      <include template="DBAZ" />
    </segment>
    <segment name="DBEZ" class="SocialInsurance.Germany.Messages.Pocos.DBEZ, SocialInsurance.Germany.Messages.Pocos" occursRef="MMEZ" collection="list">
      <include template="DBEZ" />
    </segment>
    <segment name="DBFE" class="SocialInsurance.Germany.Messages.Pocos.DBFE, SocialInsurance.Germany.Messages.Pocos" occursRef="FEAN" collection="list">
      <include template="DBFE" />
    </segment>
  </template>
</beanio>