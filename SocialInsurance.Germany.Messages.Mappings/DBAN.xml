<?xml version="1.0" encoding="utf-8" ?>
<beanio xmlns="http://www.beanio.org/2015/06">
  <template name="DBAN">
    <field name="Kennung" length="4" literal="DBAN" required="true"/>
    <field name="Länderkennzeichen" length="3" required="true"/>
    <field name="PLZ" length="10" required="true"/>
    <field name="Wohnort" length="34" required="true"/>
    <field name="Strasse" length="33" required="false"/>
    <field name="Hausnummer" length="9" required="false"/>
    <field name="Anschriftenzusatz" length="40" required="false"/>
  </template>
</beanio>