<?xml version="1.0" encoding="utf-8" ?>
<beanio xmlns="http://www.beanio.org/2015/06">
  <template name="DBNA">
    <field name="Kennung" length="4" literal="DBNA" required="true"/>
    <field name="Familienname" length="30" required="true" />
    <field name="Vorname" length="30" required="true" />
    <field name="Vorsatzwort" length="20" required="false" />
    <field name="Namenszusatz" length="20" required="false" />
    <field name="Titel" length="20" required="false" />
    <field name="Kennzeichenänderung" length="1" regex="[A]|[ ]" required="false" />
  </template>
</beanio>
