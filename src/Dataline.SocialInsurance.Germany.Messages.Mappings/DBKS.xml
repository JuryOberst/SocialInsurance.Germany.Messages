<?xml version="1.0" encoding="utf-8" ?>
<beanio xmlns="http://www.beanio.org/2016/07">
  <stream name="DBKS" format="fixedlength">
    <!--
    Wird zur Laufzeit von Typ-Konverter für DBKS benutzt.
    -->
    <record name="DBKSS" minOccurs="0">
      <field name="Kennung" length="4" literal="DBKS" required="true" rid="true"/>
      <field name="KENNZKS" length="1" literal="S" required="true" rid="true"/>
    </record>
    <record name="DBKSK" minOccurs="0">
      <field name="Kennung" length="4" literal="DBKS" required="true" rid="true"/>
      <field name="KENNZKS" length="1" literal="K" required="true" rid="true"/>
    </record>
  </stream>
</beanio>